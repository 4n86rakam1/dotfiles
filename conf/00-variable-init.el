;; 色・数値を格納する変数定義
;; 変数の先頭にv-(variable)をつける

;; ------------------------------------------------------------------------
;; @ color
;; ------------------------------------------------------------------------

;; 行番号の色
(setq v-line-number-color "#0669a3")

;; 括弧選択色
(setq v-parenthesis-color "#4F4FD9")

;; 範囲選択色
(setq v-range-selection-color "#555")

;; 行末空白色
(setq v-line-end-space-color "#b14770")

;; cursor color
(setq v-cursor-color 'Orange)

;; show parenthesis color
(setq v-show-paren-color "#ff9200")
