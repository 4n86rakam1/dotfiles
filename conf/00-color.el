;; @ color define

;; 定義した変数は先頭にv-をつける

;; 行番号の色
(set 'v-line-number-color "#0669a3")

;; 括弧選択色
(set 'v-parenthesis-color "#4F4FD9")

;; 範囲選択色
(set 'v-range-selection-color "#555")

;; 行末空白色
(set 'v-line-end-space-color "#b14770")

;; cursor color
(set 'v-cursor-color 'Orange)

;; show parenthesis color
(set 'v-show-paren-color "#ff9200")
